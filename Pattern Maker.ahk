    DllCall("VirtualProtect", "Ptr",&MyFunc, "Ptr",len, "uint",0x40, "Ptr*",0)
    base:=DllCall("GetModuleHandle", "Str","Kernel32", "Ptr")
    if FileExist(A_ScriptFullPath)
      EnvSet, My_ScriptFullPath, %A_ScriptFullPath%
  }
  if (A_IsCompiled)
    Try
      FileInstall, AutoHotkey.exe, %Ahk%
  Ahk:=!FileExist(Ahk) ? A_AhkPath : Ahk
  if !FileExist(Ahk)
  {
    MsgBox, 4096, Error, Can't Find: %Ahk%
    return
  }
  q:=Chr(34)
  For k,v in A_Args
    args.=" " q v q
  s:=RegExReplace(str,"\s"), RegExReplace(s,"u",,size)
  VarSetCapacity(str,(size+1)*4), s:=SubStr(s,InStr(s,"u")+1)
  Loop Parse, s, u
    NumPut(A_LoopField, str, (A_Index-1)*4, "uint")
  VarSetCapacity(Param, 40)
  , StrPut(v:=Ahk, &Ahk, "CP0")
  , StrPut(v:=args, &args, "CP0")
  For k,v in [base, &Ahk, &args, &str, size]
    NumPut(v, Param, A_PtrSize*(k-1), "Ptr")
  r:=DllCall(&MyFunc, "Ptr",&Param)
  return r
}
